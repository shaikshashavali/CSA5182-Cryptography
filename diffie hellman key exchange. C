#include<stdio.h>
int main()
{
	int q,p,Xa,Xb,Ya,Yb,k1,k2,a;
	printf("Enter value of q ");
	scanf("%d",&q);
	printf("Enter value of p ");
	scanf("%d",&p);
	printf("Enter value of Xa ");
	scanf("%d",&Xa);
	printf("Enter value of Xb ");
	scanf("%d",&Xb);
	a= pow(p,Xa);
	Ya=a%q;
	printf("Ya=%d",Ya);
	printf("\n");
	a=pow(p,Xb);
	Yb=a %q;
	printf("Yb=%d",Yb);
	printf("\n");
	a=pow(Yb,Xa);
	k1=a %q;
	printf("k1=%d",k1);
	printf("\n");
	a=pow(Ya,Xb);
	k2=a%q;
	printf("k2=%d",k2);
	
}
